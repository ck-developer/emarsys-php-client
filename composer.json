{
    "name": "ck-developer/emarsys-php-client",
    "type": "library",
    "description": "PHP client for emarsys",
    "keywords": [
        "emarsys",
        "api"
    ],
    "homepage": "https://github.com/ck-developer/emarsys-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Khedhiri",
            "email": "claude@khedhiri.com",
            "homepage": "http://www.khedhiri.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "php-http/httplug": "^1.1",
        "php-http/discovery": "^1.0",
        "php-http/message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.5",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/mock-client": "^1.0",
        "guzzlehttp/psr7": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Emarsys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emarsys\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
